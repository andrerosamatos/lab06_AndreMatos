package p2cg;

public class P2_GC {
	private String nome;
	private double preco;
	private int maiorScore;
	private int qntdJogadas;
	private boolean zerou;
	private int score;
	
	
	
	public void Jogo(String nome, double preco, int maiorScore, int qntdJogadas, boolean zerou){
	//	this.jogo = new Jogo(nome, preco);
		this.maiorScore = maiorScore;
		this.qntdJogadas = qntdJogadas;
		this.zerou = zerou;
	}


	public void registraJogada(int score, boolean zerou) {
		return int x2p;
		if (this.score > maiorScore){
			maiorScore = this.score;
		}
		
	}

	
	
}
